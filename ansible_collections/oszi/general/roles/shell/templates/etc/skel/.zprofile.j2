# {{ ansible_managed }}
# shellcheck shell=sh disable=SC1090 # non-constant source

# See also /etc/{zsh/,}zprofile from the distribution.
# Sequence: zshenv > zprofile* > zshrc* > zlogin > zlogout

if [ -f ~/.profile ]; then
    . ~/.profile
fi
{% if ansible_facts.os_family == 'Debian' %}

# See /etc/zsh/zshrc vs {{ shell_shrc_d_safe_path }}/01-key-bindings.zsh
export DEBIAN_PREVENT_KEYBOARD_CHANGES=1
{% endif %}
